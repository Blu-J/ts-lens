{
  "name": "ts-lens",
  "version": "1.6.3",
  "description": "Simple typescript get/set lens",
  "files": [
    "lib/"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/blu-j/ts-lens"
  },
  "author": "Justin Miller<mogulslayer@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --outDir lib/ ",
    "test": "jest",
    "ci": "npm run lint && npm run test && npm run pretty-check",
    "lint": "tslint --project tsconfig.json --format stylish",
    "pretty-check": "prettier --check src/**",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^20.1.3",
    "jest": "^26.1.0",
    "prettier": "^2.0.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^4.0.2"
  },
  "prettier": {},
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(lib|src)/.*\\.spec\\.(js|ts)x?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "ts-jest": "^26.1.1"
  }
}
